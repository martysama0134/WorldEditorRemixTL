        MENUITEM SEPARATOR
        MENUITEM "Ajuster la perspective",      ID_VIEW_DLG_PERSPECTIVE
    END
    POPUP "Info"
    BEGIN
        MENUITEM "A propos du WorldEditor...",  ID_APP_ABOUT
    END
END


